{
  "name": "node",
  "version": "1.0.0",
  "main": "dist/app.js",
  "type": "module",
  "private": "true",
  "scripts": {
    "dev": "bun --watch src/app.ts",
    "build": "rimraf dist && tsup && tsc-alias -p ./tsconfig.json",
    "lint": "eslint -c eslint.config.mjs",
    "typeorm": "typeorm-ts-node-esm -d ./src/database/dataSource.ts",
    "schema:sync": "bun run typeorm -- schema:sync",
    "typeorm:cache": "bun run typeorm -- cache:clear",
    "schema:drop": "bun run typeorm -- schema:drop",
    "migration:generate": "bun run typeorm -- migration:generate ./src/database/migration/schema-update",
    "migration:show": "bun run typeorm -- migration:show",
    "migration:run": "bun run typeorm -- migration:run",
    "migration:revert": "bun run typeorm -- migration:revert"
  },
  "tsup": {
    "platform": "node",
    "format": "esm",
    "entry": ["src/**/*.ts"],
    "outDir": "dist/mjs/src",
    "tsconfig": "./tsconfig.json",
    "bundle": false,
    "minify": true,
    "minifyIdentifiers": true,
    "minifySyntax": true,
    "minifyWhitespace": true,
    "skipNodeModulesBundle": true,
    "splitting": true,
    "clean": true,
    "dts": false
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.8.4",
    "@types/sharp": "^0.32.0",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "dependencies": {
    "@fastify/cookie": "9.4.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/passport": "2.5.0",
    "@fastify/session": "10.9.0",
    "@fastify/static": "7.0.4",
    "@fastify/websocket": "^10.0.1",
    "bcrypt": "^5.1.1",
    "bull": "4.16.3",
    "chalk": "^5.3.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "fastify": "4.28.1",
    "glob": "11.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "nanoid": "^5.0.8",
    "pino-pretty": "^11.3.0",
    "puppeteer": "^23.6.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  }
}